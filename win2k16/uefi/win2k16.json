      "boot_wait": "1s",
      "boot_command": [
        "<esc>"
      ],
      "headless": true,
      "vnc_bind_address": "10.119.1.17",
      "vnc_use_password": true,
      "communicator": "winrm",
      "winrm_username": "packer",
      "winrm_password": "packer",
      "winrm_use_ssl": "false",
      "winrm_insecure": "true",
      "winrm_timeout": "1h",
